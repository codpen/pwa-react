{
  "name": "pwa-kit-react-sdk",
  "version": "1.4.0-dev",
  "description": "A library that supports the isomorphic React rendering pipeline for Commerce Cloud Managed Runtime apps",
  "main": "dist/index.js",
  "bin": {
    "sdk-pwa-build": "bin/pwa-build.js",
    "sdk-upload": "bin/upload.js",
    "sdk-create-hash-manifest": "bin/create-hash-manifest.js"
  },
  "engines": {
    "node": "^12.0.0 || ^14.0.0"
  },
  "files": [
    "bin",
    "LICENSE",
    "scripts",
    "ssr",
    "TERMS_OF_USE.md",
    "utils",
    "webpack"
  ],
  "publishConfig": {
    "directory": "dist"
  },
  "scripts": {
    "prepare": "npm run prod:build",
    "prod:build": "cross-env NODE_ENV=production internal-lib-build build",
    "dev:watch": "watch 'npm run dev:build' src/",
    "lint": "npm run lint:js",
    "lint:fix": "npm run lint:js -- --fix",
    "lint:js": "internal-lib-build lint '\"**/*.{js,jsx}\"'",
    "test": "internal-lib-build test",
    "test:watch": "npm test -- --watch",
    "test:inspect": "node --inspect-brk jest --runInBand",
    "version": "node ./scripts/version.js",
    "format": "internal-lib-build format '\"**/*.{js,jsx}\"'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SalesforceCommerceCloud/pwa-kit.git"
  },
  "homepage": "https://github.com/SalesforceCommerceCloud/pwa-kit/tree/develop/packages/pwa-kit-react-sdk#readme",
  "bugs": {
    "url": "https://github.com/SalesforceCommerceCloud/pwa-kit/issues"
  },
  "author": "dev@mobify.com",
  "license": "SEE LICENSE IN LICENSE",
  "dependencies": {
    "@loadable/babel-plugin": "^5.13.2",
    "@loadable/server": "^5.15.0",
    "@loadable/webpack-plugin": "^5.15.0",
    "archiver": "1.3.0",
    "aws-lambda-test-utils": "^1.3.0",
    "aws-sdk": "^2.984.0",
    "aws-serverless-express": "3.3.5",
    "bluebird": "^3.5.1",
    "body-parser": "^1.19.0",
    "chalk": "1.1.3",
    "commander": "^8.3.0",
    "compression": "1.7.4",
    "cross-env": "^5.2.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "1.15.2",
    "event-emitter": "^0.3.5",
    "express": "^4.17.1",
    "git-rev-sync": "^3.0.1",
    "glob": "7.1.1",
    "header-case": "1.0.1",
    "hoist-non-react-statics": "^3.3.1",
    "http-proxy-middleware": "0.21.0",
    "ignore-loader": "^0.1.2",
    "lodash": "^4.17.20",
    "merge-descriptors": "^1.0.1",
    "mime-types": "2.1.32",
    "minimatch": "3.0.4",
    "mkdirp": "^1.0.4",
    "morgan": "1.9.1",
    "nock": "^13.1.1",
    "node-fetch": "2.6.1",
    "node-html-parser": "^3.3.4",
    "open": "^8.4.0",
    "prop-types": "^15.6.0",
    "pwa-kit-build": "^1.4.0-dev",
    "pwa-kit-runtime": "^1.4.0-dev",
    "raf": "^3.4.0",
    "react-uid": "^2.2.0",
    "replace-in-file": "^6.2.0",
    "request": "^2.88.0",
    "resolve-from": "^5.0.0",
    "rimraf": "2.6.1",
    "s3rver": "^3.1.0",
    "semver": "^7.3.2",
    "serialize-javascript": "^6.0.0",
    "set-cookie-parser": "^2.2.1",
    "shelljs": "^0.8.4",
    "sinon": "2.3.2",
    "superagent": "^6.1.0",
    "supertest": "^4.0.2",
    "svg-sprite-loader": "^6.0.11",
    "ua-parser-js": "^0.7.24",
    "url-parse": "^1.4.7",
    "validator": "^13.7.0",
    "watch": "1.0.1",
    "whatwg-encoding": "^1.0.5",
    "yargs": "15.4.1"
  },
  "devDependencies": {
    "@loadable/component": "^5.15.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.6",
    "internal-lib-build": "^1.4.0-dev",
    "regenerator-runtime": "^0.13.9",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "5.2.0",
    "react-router-dom": "^5.1.2"
  },
  "peerDependencies": {
    "@loadable/component": "^5.15.0",
    "react": ">=16.14 || <18",
    "react-dom": ">=16.14 || <18",
    "react-helmet": "6",
    "react-router-dom": "^5.1.2"
  }
}
