  },
  "comments": {
    "fsevents": "fsevents@1.2.9 is locked in to prevent broken builds on windows for v1.2.11"
  },
  "scripts": {
    "analyze-build": "cross-env MOBIFY_ANALYZE=true npm run prod:build",
    "lint": "npm run lint:js",
    "lint:fix": "npm run lint:js -- --fix",
    "lint:js": "eslint '**/*.{js,jsx}'",
    "push": "cross-env NODE_ENV=production npm run prod:build && sdk-upload",
    "save-credentials": "sdk-save-credentials",
    "start": "cross-env NODE_ICU_DATA=node_modules/full-icu node ./scripts/start ssr",
    "start:inspect": "npm run start -- --inspect",
    "start:pseudolocale": "npm run compile-messages:pseudo && cross-env TARGET_LOCALE=pseudo npm run start",
    "prod:build": "npm run compile-messages && cross-env NODE_ENV=production sdk-pwa-build",
    "test": "cross-env NODE_ICU_DATA=node_modules/full-icu jest --maxWorkers=2 --env=jest-environment-jsdom-sixteen",
    "test:e2e": "cypress run",
    "test:e2e-ci": "node ./scripts/run-e2e.js",
    "test:integration": "cross-env TEST_TYPE=integration npm run test",
    "test:lighthouse": "lhci autorun --config=tests/lighthouserc.js",
    "test:max-file-size": "bundlesize",
    "format": "prettier --write \"**/*.{js,jsx}\"",
    "extract-messages": "formatjs extract 'app/**/*.{js,jsx}' --out-file app/translations/$npm_package_l10n_defaultLocale.json",
    "compile-messages": "formatjs compile-folder --ast app/translations app/translations/compiled",
    "compile-messages:pseudo": "formatjs compile --ast app/translations/$npm_package_l10n_defaultLocale.json --out-file app/translations/compiled/pseudo.json --pseudo-locale xx-LS"
  },
  "bundlesize": [
    {
      "path": "build/main.js",
      "maxSize": "22 kB"
    },
    {
      "path": "build/vendor.js",
      "maxSize": "250 kB"
    }
  ],
  "browserslist": [
    "iOS >= 9.0",
    "Android >= 4.4.4",
    "last 4 ChromeAndroid versions"
  ],
  "l10n": {
    "supportedLocales": [
      "en",
      "fr"
    ],
    "defaultLocale": "en"
  }
}
