{
    "name": "hello-world",
    "version": "2.0.0-dev",
    "license": "See license in LICENSE",
    "private": true,
    "devDependencies": {
        "pwa-kit-build": "^1.4.0-dev",
        "pwa-kit-react-sdk": "^2.0.0-dev",
        "pwa-kit-runtime": "^1.4.0-dev",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-helmet": "^6.1.0",
        "react-router-dom": "^5.1.2"
    },
    "scripts": {
        "test": "mrt test",
        "format": "mrt format '\"**/*.{js,jsx}\"'",
        "lint": "mrt lint '\"**/*.{js,jsx}\"'",
        "lint:fix": "npm run lint -- --fix",
        "start": "mrt start",
        "prod:build": "mrt build",
        "push": "mrt push",
        "save-credentials": "mrt save-credentials"
    },
    "mobify": {
        "ssrEnabled": true,
        "ssrParameters": {
            "ssrFunctionNodeVersion": "14.x"
        },
        "ssrOnly": [
            "ssr.js",
            "node_modules/**/*.*"
        ],
        "ssrShared": [
            "intentionally-does-not-exist"
        ]
    }
}
